<p id="notice"><%= notice %></p>

<h1>Tag To Snips</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Snip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_to_snips.each do |tag_to_snip| %>
      <tr>
        <td><%= tag_to_snip.tag_id %></td>
        <td><%= tag_to_snip.snip_id %></td>
        <td><%= link_to 'Show', tag_to_snip %></td>
        <td><%= link_to 'Edit', edit_tag_to_snip_path(tag_to_snip) %></td>
        <td><%= link_to 'Destroy', tag_to_snip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag To Snip', new_tag_to_snip_path %>
